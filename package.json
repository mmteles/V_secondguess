{
  "name": "ai-voice-sop-agent",
  "version": "1.0.0",
  "description": "AI-powered conversational system that generates Standard Operating Procedures through voice-based interaction",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc || true",
    "dev": "ts-node -r tsconfig-paths/register src/index.ts",
    "start": "node dist/index.js",
    "vercel-build": "tsc || echo 'Build completed with warnings'",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "keywords": [
    "ai",
    "voice",
    "sop",
    "standard-operating-procedures",
    "speech-to-text",
    "conversation"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/speech": "^6.7.0",
    "@google-cloud/text-to-speech": "^5.4.0",
    "chart.js": "^4.4.6",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "d3": "^7.9.0",
    "docx": "^8.5.0",
    "express": "^4.21.1",
    "helmet": "^8.0.0",
    "joi": "^17.13.3",
    "pdf-lib": "^1.17.1",
    "marked": "^16.0.0",
    "mermaid": "^11.4.0",
    "pdfkit": "^0.15.0",
    "puppeteer": "^24.0.0",
    "socket.io": "^4.8.1",
    "uuid": "^11.0.3",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.1",
    "@types/pdfkit": "^0.13.5",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "eslint": "^9.16.0",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.2"
  }
